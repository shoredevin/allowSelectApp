{
    "manifest_version": 3,
    "name": "Allow Select",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*", "https://*/*"
            ],
            "js": [
                "./scripts.js"
            ]
        }
    ]
}